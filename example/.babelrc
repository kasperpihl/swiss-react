{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "root": ["."],
        "alias": {
          "swiss-react": "../dist/es/index"
        }
      }
    ],
    "react-hot-loader/babel",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}

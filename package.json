{
  "name": "swiss-react",
  "version": "0.4.7",
  "description": "CSS-in-js with the best of the react and sass world together.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "node ./tools/build.js",
    "start": "npm-run-all --parallel start:swiss start:server",
    "start:swiss": "BABEL_ENV=es babel src -d dist/es --watch",
    "start:server": "cd ./example && npm install && npm start",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swipesapp/swiss-react.git"
  },
  "author": "Kasper Pihl Tornoe",
  "license": "MIT",
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "styles",
    "sass",
    "scss",
    "less"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "gzip-size": "^5.0.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "pretty-bytes": "^5.1.0",
    "react": "^16.6.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "uglify-es": "^3.3.9"
  },
  "peerDependencies": {
    "react": ">=16.6"
  }
}

{
  "name": "swiss-react",
  "version": "0.3.4",
  "description": "CSS-in-js with the best of the react and sass world together.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "node ./tools/build.js",
    "start": "npm-run-all --parallel start:swiss start:server",
    "start:swiss": "BABEL_ENV=es babel src -d dist/es --watch",
    "start:server": "cd ./example && npm install && npm start",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swipesapp/swiss-react.git"
  },
  "author": "Kasper Pihl Tornoe",
  "license": "MIT",
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "styles",
    "sass",
    "scss",
    "less"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "gzip-size": "^5.0.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "pretty-bytes": "^5.1.0",
    "react": "^16.5.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "uglify-es": "^3.3.9"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {}
}

{
  "name": "swiss-react",
  "version": "0.4.15",
  "description": "CSS-in-js with the best of the react and sass world together.",
  "source": "src2/index.ts",
  "main": "dist/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "build": "rimraf dist && microbundle --jsx React.createElement --format cjs --no-source-map",
    "watch": "npm run build -- --watch",
    "start": "rimraf dist && parcel demo/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swipesapp/swiss-react.git"
  },
  "alias": {
    "swiss-react": "./src2/index.ts"
  },
  "author": "Kasper Pihl Tornoe",
  "license": "MIT",
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "styles",
    "sass",
    "scss",
    "less"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.9.4",
    "microbundle": "^0.12.0-next.8",
    "parcel-bundler": "^1.12.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "tools": "0.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "dependencies": {
    "csstype": "^2.6.7"
  }
}
